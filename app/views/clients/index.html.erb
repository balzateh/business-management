<div class="row mb-5">
    <div class="col-md-10">
        <h3>Listado de Clientes</h3>
    </div>
    <div class="col-md-2">
        <a class="btn btn-primary" href="<%= new_client_path %>" role="button">Nuevo Cliente</a>
    </div>
</div>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre Cliente</th>
      <th scope="col">Nombre Empresa</th>
      <th scope="col">Correo Electr√≥nico</th>
      <th scope="col">Telefono</th>
      <th scope="col">Sector Economico</th>
      <th scope="col">Detalles</th>
    </tr>
  </thead>
  <tbody>
    <% @clients.each do |client| %>
    <tr>
      <th scope="row"><%= client.id %></th>
      <td><%= client.name %></td>
      <td><%= client.company %></td>
      <td><%= client.email %></td>
      <td><%= client.phone %></td>
      <td><%= client.sector_name %></td>
      <td><%= link_to 'Ver', client_path(client) %></td>
    </tr>
    <% end %>
  </tbody>
</table>